<html>

<title><%= title %></title>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
<meta name="description" content="">
<meta name="author" content="">
<link rel="stylesheet" href="bootstrap/css/bootstrap.min.css">
<link rel="stylesheet" href="stylesheets/style.css">

<body ng-app="app" ng-controller="APIController">
    <% include navbar %>

    <ul>
        <li ng-repeat="endpoint in apidoc">
            <strong>{{endpoint.endpoint}}</strong>
            {{endpoint.description}}
            <ul ng-if="endpoint.endpoints">
                <li ng-repeat="inner_endpoint in endpoint.endpoints">
                    <strong>{{inner_endpoint.endpoint}}</strong>
                    {{inner_endpoint.description}}
                    <ul ng-if="inner_endpoint.endpoints">
                        <li ng-repeat="inside_inner_endpoints in inner_endpoint.endpoints">
                            <strong>{{inside_inner_endpoints.endpoint}}</strong>
                            {{inside_inner_endpoints.description}}
                        </li>
                    </ul>
                </li>
            </ul>
        </li>
    </ul>

</body>

<script src="jquery/jquery.min.js"></script>
<script src="bootstrap/js/bootstrap.min.js"></script>
<script src="angular/angular.min.js"></script>
<script src="angular-cookies/angular-cookies.min.js"></script>
<script src="javascripts/app.js"></script>
<script src="javascripts/service/UserTokenFactory.js"></script>
<script src="javascripts/service/UserService.js"></script>
<script src="javascripts/controller/LoginController.js"></script>
<script src="javascripts/controller/NavbarController.js"></script>
<script src="javascripts/controller/APIController.js"></script>
</html>